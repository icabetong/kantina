{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"semi": false,
	"bracketSameLine": true,
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
	"importOrder": ["^svelte-(.*)$", "^@(.*)$", "^\\$(.*)$", "^[./]"],
	"importOrderSortSpecifiers": true
}
